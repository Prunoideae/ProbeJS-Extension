{
  "name": "probejs",
  "displayName": "ProbeJS",
  "description": "Supplying Rich Completion Display for ProbeJS",
  "version": "0.0.8",
  "engines": {
    "vscode": "^1.78.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "probejs.reload",
        "title": "ProbeJS: Reload Item Attributes"
      },
      {
        "command": "probejs.populateLang",
        "title": "ProbeJS: Populate Unlocalized Strings"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.78.0",
    "@typescript-eslint/eslint-plugin": "^5.59.1",
    "@typescript-eslint/parser": "^5.59.1",
    "@vscode/test-electron": "^2.3.0",
    "eslint": "^8.39.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^5.0.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Prunoideae/ProbeJS-Extension"
  },
  "license": "lgpl2.1",
  "icon": "icon.png",
  "publisher": "Prunoideae",
  "dependencies": {
    "@octokit/rest": "^20.0.2"
  }
}